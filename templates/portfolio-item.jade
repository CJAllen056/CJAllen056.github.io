extends layout

block append vars
  -bodyclass = "portfolio-detail"

block prepend title
  | #{ page.title + " - " }

block header
  h1 = page.title

block content
  article.portfolio-info
    section.content!= typogr(page.html).typogrify()
    
block prepend footer
  div.nav
    a(href=contents.index.url) &laquo; Back
